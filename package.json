{
  "name": "@blackglory/aho-corasick",
  "version": "0.1.7",
  "description": "A simple Node.js wrapper for Rust's aho-corasick package",
  "keywords": [
    "aho-corasick",
    "rust",
    "native"
  ],
  "files": [
    "lib",
    "native/src",
    "native/build.rs",
    "native/Cargo.lock",
    "native/Cargo.toml"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": "git@github.com:BlackGlory/aho-corasick.git",
  "author": "BlackGlory <woshenmedoubuzhidao@blackglory.me>",
  "license": "MIT",
  "scripts": {
    "install": "neon build --release && rimraf native/target",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --quiet src __tests__",
    "test": "jest --config jest.config.js",
    "test:coverage": "jest --coverage --config jest.config.js",
    "prepublishOnly": "run-s clean build",
    "clean": "run-p clean:*",
    "clean:rs": "neon clean",
    "clean:ts": "rimraf lib",
    "build": "run-p build:*",
    "build:rs": "neon build --release",
    "build:ts": "run-s build:ts:*",
    "build:ts:compile": "tsc --project tsconfig.build.json --module commonjs --target es2018 --outDir lib",
    "build:ts:patch": "tscpaths -p tsconfig.build.json -s ./src -o ./lib",
    "benchmark": "./benchmark/benchmark.sh",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s lint build test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@blackglory/go": "^0.1.0",
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.10.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "extra-filesystem": "^0.3.10",
    "husky": "4",
    "jest": "^27.2.5",
    "npm-run-all": "^4.1.5",
    "pretty-bytes": "^5.6.0",
    "standard-version": "^9.3.1",
    "ts-jest": "^27.0.5",
    "tscpaths": "^0.0.9",
    "tslib": "^2.3.1",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "neon-cli": "^0.9.1",
    "rimraf": "^3.0.2"
  }
}
